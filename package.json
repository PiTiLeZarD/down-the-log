{
    "name": "down-the-log",
    "version": "0.0.1",
    "main": "src/AppEntry.tsx",
    "homepage": "https://pitilezard.github.io/down-the-log",
    "scripts": {
        "start:all": "expo start -c",
        "start:android": "expo start -c --android",
        "start:ios": "expo start -c --ios",
        "start:web": "expo start -c --web",
        "start:tauri": "tauri dev",
        "build:all": "expo export",
        "build:android": "expo export -p android",
        "build:ios": "expo export -p iod",
        "build:web": "expo export -p web",
        "build:tauri": "tauri build",
        "types": "tsc --noEmit",
        "clean": "rm -Rf dist",
        "prepages": "yarn clean && yarn build:web && sed -i -e 's/bundles/down-the-log\\/bundles/g' ./dist/index.html && sed -i -e 's/assets/down-the-log\\/assets/g' ./dist/bundles/*.js",
        "pages": "gh-pages -b ghpages -d dist"
    },
    "dependencies": {
        "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
        "@expo-google-fonts/source-sans-pro": "^0.2.3",
        "@expo/html-elements": "latest",
        "@expo/vector-icons": "^13.0.0",
        "@legendapp/motion": "latest",
        "@react-native-async-storage/async-storage": "1.17.11",
        "@react-native-picker/picker": "2.4.8",
        "@react-native/normalize-colors": "^0.74.1",
        "@react-navigation/drawer": "^6.6.6",
        "@react-navigation/native": "^6.1.9",
        "@types/luxon": "^3.3.3",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "expo": "~48.0.21",
        "expo-device": "~5.2.1",
        "expo-font": "~11.1.1",
        "expo-location": "~15.1.1",
        "expo-splash-screen": "~0.18.2",
        "expo-status-bar": "~1.4.4",
        "gh-pages": "^6.0.0",
        "luxon": "^3.4.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-dropzone": "^14.2.3",
        "react-hook-form": "^7.47.0",
        "react-native": "0.71.14",
        "react-native-aria": "^0.2.3",
        "react-native-gesture-handler": "~2.9.0",
        "react-native-picker-select": "^9.0.0",
        "react-native-reanimated": "~2.14.4",
        "react-native-safe-area-context": "4.5.0",
        "react-native-screens": "~3.20.0",
        "react-native-svg": "13.4.0",
        "react-native-unistyles": "^1.2.0",
        "react-native-uuid": "^2.0.1",
        "react-native-web": "~0.18.10",
        "zustand": "^4.4.5"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@expo/webpack-config": "^18.1.3",
        "@tauri-apps/cli": "^1.5.6",
        "@types/react": "~18.0.38",
        "typescript": "^4.9.4"
    },
    "private": true
}
