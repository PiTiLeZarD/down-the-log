{
    "name": "down-the-log",
    "version": "0.0.1",
    "main": "expo-router/entry",
    "homepage": "/down-the-log",
    "scripts": {
        "start": "expo start -c --port 19000",
        "start:all": "yarn start",
        "start:android": "yarn start --android",
        "start:ios": "yarn start --ios",
        "start:web": "yarn start --web",
        "start:tauri": "tauri dev",
        "build": "expo export --output-dir ./down-the-log",
        "build:all": "yarn build -c",
        "build:android": "yarn build -p android",
        "build:ios": "yarn build -p ios",
        "build:web": "yarn build -p web",
        "build:tauri": "tauri build",
        "eas:dev:ios": "eas build --profile ios-simulator --platform ios",
        "eas:dev:android": "eas build --profile android-simulator --platform android",
        "eas:dev:androidapk": "eas build --profile android-apk --platform android",
        "eas:run:ios": "eas build:run -p ios",
        "eas:start": "yarn start --dev-client",
        "types": "tsc --noEmit",
        "clean": "rm -Rf down-the-log",
        "prepages": "yarn clean && yarn build:web",
        "pages": "gh-pages -t -b ghpages -d down-the-log",
        "refreshpota": "wget -O scripts/all_parks_ext.csv https://pota.app/all_parks_ext.csv && ts-node scripts/pota.ts",
        "refreshsota": "wget -O scripts/summitslist.csv https://storage.sota.org.uk/summitslist.csv && ts-node scripts/sota.ts",
        "refreshwwff": "wget -O scripts/wwff_directory.csv https://wwff.co/wwff-data/wwff_directory.csv && ts-node scripts/wwff.ts",
        "refreshdata": "yarn refreshpota && yarn refreshsota && yarn refreshwwff"
    },
    "dependencies": {
        "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
        "@expo-google-fonts/source-sans-pro": "^0.2.3",
        "@expo/html-elements": "0.9.1",
        "@expo/metro-runtime": "~3.1.3",
        "@expo/vector-icons": "^14.0.0",
        "@legendapp/motion": "^2.3.0",
        "@react-native-async-storage/async-storage": "1.21.0",
        "@react-native-picker/picker": "2.6.1",
        "@react-native/normalize-colors": "^0.74.1",
        "@types/luxon": "^3.4.2",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "axios": "^1.6.7",
        "expo": "~50.0.6",
        "expo-constants": "^15.4.5",
        "expo-dev-client": "~3.3.8",
        "expo-device": "~5.9.3",
        "expo-font": "~11.10.2",
        "expo-image": "~1.10.6",
        "expo-linking": "^6.2.2",
        "expo-location": "~16.5.3",
        "expo-router": "^3.4.7",
        "expo-splash-screen": "~0.26.4",
        "expo-status-bar": "^1.11.1",
        "gh-pages": "^6.1.1",
        "humanize-duration": "^3.31.0",
        "jssha": "^3.3.1",
        "luxon": "^3.4.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-dropzone": "^14.2.3",
        "react-hook-form": "^7.50.0",
        "react-native": "0.73.6",
        "react-native-aria": "^0.2.3",
        "react-native-big-list": "^1.6.1",
        "react-native-gesture-handler": "~2.14.0",
        "react-native-picker-select": "^9.0.1",
        "react-native-reanimated": "~3.6.2",
        "react-native-safe-area-context": "4.8.2",
        "react-native-screens": "~3.29.0",
        "react-native-svg": "14.1.0",
        "react-native-svg-charts": "^5.4.0",
        "react-native-unistyles": "^2.2.0",
        "react-native-uuid": "^2.0.1",
        "react-native-web": "~0.19.10",
        "sweetalert2": "^11.10.5",
        "zustand": "^4.5.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.9",
        "@expo/webpack-config": "~19.0.1",
        "@tauri-apps/cli": "^1.5.9",
        "@types/humanize-duration": "^3.27.4",
        "@types/react": "~18.2.54",
        "@types/react-native-svg-charts": "^5.0.16",
        "csv-parse": "^5.5.3",
        "fast-xml-parser": "^4.3.4",
        "typescript": "^5.1.3"
    },
    "private": true
}
