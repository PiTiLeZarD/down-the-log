{
    "name": "down-the-log",
    "version": "0.0.1",
    "main": "src/AppEntry.tsx",
    "homepage": "https://pitilezard.github.io/down-the-log",
    "scripts": {
        "start": "expo start -c --port 19000",
        "start:all": "yarn start",
        "start:android": "yarn start --android",
        "start:ios": "yarn start --ios",
        "start:web": "yarn start --web",
        "start:tauri": "tauri dev",
        "build": "expo export",
        "build:all": "yarn build",
        "build:android": "yarn build -p android",
        "build:ios": "yarn build -p ios",
        "build:web": "yarn build -p web",
        "build:tauri": "tauri build",
        "eas:dev:ios": "eas build --profile ios-simulator --platform ios",
        "eas:dev:android": "eas build --profile android-simulator --platform android",
        "eas:dev:androidapk": "eas build --profile android-apk --platform android",
        "eas:run:ios": "eas build:run -p ios",
        "eas:start": "yarn start --dev-client",
        "types": "tsc --noEmit",
        "clean": "rm -Rf dist",
        "prepages": "yarn clean && yarn build:web && sed -i -e 's/bundles/down-the-log\\/bundles/g' ./dist/index.html && sed -i -e 's/assets/down-the-log\\/assets/g' ./dist/bundles/*.js",
        "pages": "gh-pages -b ghpages -d dist"
    },
    "dependencies": {
        "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
        "@expo-google-fonts/source-sans-pro": "^0.2.3",
        "@expo/html-elements": "latest",
        "@expo/vector-icons": "^13.0.0",
        "@legendapp/motion": "latest",
        "@react-native-async-storage/async-storage": "1.18.2",
        "@react-native-picker/picker": "2.4.10",
        "@react-native/normalize-colors": "^0.74.1",
        "@react-navigation/native": "^6.1.9",
        "@react-navigation/stack": "^6.3.20",
        "@types/luxon": "^3.3.3",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "axios": "^1.6.5",
        "expo": "^49.0.21",
        "expo-dev-client": "~2.4.12",
        "expo-device": "~5.4.0",
        "expo-font": "~11.4.0",
        "expo-image": "~1.3.5",
        "expo-location": "~16.1.0",
        "expo-splash-screen": "~0.20.5",
        "expo-status-bar": "~1.6.0",
        "gh-pages": "^6.0.0",
        "jssha": "^3.3.1",
        "luxon": "^3.4.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-dropzone": "^14.2.3",
        "react-hook-form": "^7.47.0",
        "react-native": "0.72.6",
        "react-native-aria": "^0.2.3",
        "react-native-big-list": "^1.6.1",
        "react-native-gesture-handler": "~2.12.0",
        "react-native-picker-select": "^9.0.0",
        "react-native-reanimated": "~3.3.0",
        "react-native-safe-area-context": "4.6.3",
        "react-native-screens": "~3.22.0",
        "react-native-svg": "13.9.0",
        "react-native-svg-charts": "^5.4.0",
        "react-native-unistyles": "^2.0.0",
        "react-native-uuid": "^2.0.1",
        "react-native-web": "~0.19.6",
        "sweetalert2": "^11.10.3",
        "zustand": "^4.4.5"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@expo/webpack-config": "^19.0.0",
        "@tauri-apps/cli": "^1.5.6",
        "@types/react": "~18.2.14",
        "@types/react-native-svg-charts": "^5.0.16",
        "csv-parse": "^5.5.3",
        "fast-xml-parser": "^4.3.3",
        "typescript": "^5.1.3"
    },
    "private": true
}
